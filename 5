def ranges(a, nam):
    max_nam = max(a)
    if nam > max_nam:
        a.insert(0, nam)
    elif nam in a:
        a.insert(-a[::-1].index(nam), nam)
    else:
        a.append(nam)


my_list = [7, 5, 4, 3]

print(my_list)
user_number = int(input('Введите натуральное число:'))
ranges(my_list, user_number)
print(my_list)
