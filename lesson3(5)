s = 0
is_exit = False
while True:
    number = input("Введите последовательность целых чисел через пробел, для выхода q:").split()
    for item in number:
        if item == "q":
            is_exit = True
            break
        try:
            item = int(item)
        except ValueError:
            pass
        s += item
    print("сумма:", s)
    if is_exit:
        break
